function mouseOverAsh() {
  document.getElementById("wholeTable").style.backgroundImage = 'url("imageTableAsh.jpg")';
  document.getElementById("wholeTable").style.backgroundSize = "cover";
  document.getElementById("wholeTable").style.backgroundRepeat = "no-repeat";
  document.getElementById("wholeTable").style.backgroundPosition = "center";
}

function mouseOutAsh() {
  document.getElementById("wholeTable").style.backgroundImage = "";
}

